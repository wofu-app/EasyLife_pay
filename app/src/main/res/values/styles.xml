<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="bt_main">
        <item name="android:layout_width">166px</item>
        <item name="android:layout_height">166px</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bt_main_bg</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:textColor">#fff</item>
    </style>
    <!-- 继续按钮样式 -->
    <style name="continue_button_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/selector_background_ack</item>
        <item name="android:gravity">center</item>
        <item name="android:src">@mipmap/go_continue</item>
    </style>
</resources>
